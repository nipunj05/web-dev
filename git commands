# initialize git repository
git init

#check the status of file
git status

#configuration of email and username 
git config user.name "yourusername"
git config user.email "youremail@email.com"

#staging the files
git add filename,filename2
# or
git add .

#one-time step
git remote add origin "http://url_of_your_git_repo.git"

# comit the changes (important)
git commit -m "any message stating for what this is"

# choose branch
git checkout ,aster
#or
git checkout -b nyi_branch

# push the changes to your branch
git push origin nayi_branch
